{
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "require": {
    "illuminate/container": "^9.32",
    "twig/intl-extra": "^3.4",
    "twig/twig": "^3.0",
    "vlucas/phpdotenv": "^5.4",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^6.1"
  }
}
